body
{
    background: url("images/bg_squares.png") repeat fixed 0 0 transparent;
    position: relative;
    width: 100%;
    color: #333333;
    font: 12px/1.5 sans-serif;
    margin: 0;
    line-height: 1;
    padding: 0;
}
#topbar
{
    height: 77px;
    margin-bottom: 10px;
}
.logo
{
    background: url("images/logo.png") no-repeat 6px 10px;
    margin-left: 3px;
    float: left;
    height: 87px;
    width: 280px;
    display: block;
}
a img { border:0; }
.logo span
{
    display: none;
}
#topbar .user_box
{
    float: right;
    margin-bottom: 0;
    margin-right: 9px;
    margin-top: 10px;
    padding: 5px;
    width: 250px;
    color: #FFFFFF;
}
.user_box img
{
    border: 1px solid #333333;
    display: block;
    float: left;
    margin-bottom: 0;
    margin-right: 8px;
    padding: 0;
}
.user_box h2
{
    color: #FFFFFF;
    float: left;
    font-size: 13px;
    line-height: 12px;
    margin-bottom: 7px;
    margin-top: 5px;
}
.user_box h3
{
    color: #CCCCCC;
    float: left;
    font-size: 12px;
    font-weight: normal;
    margin-bottom: 7px;
    margin-top: 0;
}
.user_box ul
{
    float: left;
    margin: 0;
    padding: 0;
}
.user_box ul li
{
    display: inline;
    float: left;
    list-style: none outside none;
    margin: 0;
    position: relative;
}
.user_box ul li a
{
    color: #AAAAAA;
    font-size: 11px;
}
.user_box ul li span
{
    color: #666666;
    margin: 0 4px;
}
#topbar h3 a
{
    color: #AB65B8;
    text-shadow: 0 1px 1px #000000;
}
.user_box a
{
    text-decoration: none;
}
body .main_container
{
    margin: 10px !important;
}
#wrapper {
	padding-bottom: 50px;
}
div.main_container
{
    background: url("images/grey_noise.png") repeat scroll 0 0 #E5E8EA;
    border-radius: 4px 4px 4px 4px;
    display: block;
    margin: 10px 10px 10px 196px;
    padding: 20px 10px 5px;
    position: relative;
    width: auto;
}


.isolate{
	position: relative;
}
.isolate .center{
	top:200px;
}

.isolate div.main_container {
	background: rgba(0,0,0,0.3);
	padding: 20px;
	margin: 10px;
	position: relative;
	border: 1px solid #000;
}


.isolate #loading_overlay .loading_message{
	margin-left: 0;
}

.isolate .section {
	padding: 10px !important;
}

.isolate .section :last-child {
	margin-bottom: 0px !important;
}

.isolate .box {
	margin: 0 !important;
}

.isolate .block {
	border: 0 !important;
}


.dashed_placeholder {
	display: inline-block;
	margin: -0px 0.9% 0px;
	background: #eee;
	outline: 1px dashed #bbb;
}

.flat_area
{
    margin-bottom: 15px;
    margin-top: 0;
    opacity: 1;
    margin-left: 0.99%;
    margin-right: 0.99%;
}
.flat_area h2
{
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-image: url("images/trans_black_less.png");
    border-color: #FFFFFF #F8F8F8;
    border-style: solid;
    border-width: 1px;
    font-weight: bold;
    line-height: 30px;
    margin: 0 0 10px;
    padding: 2px 10px;
    text-indent: 0;
    border-radius: 2px 2px 2px 2px;
    text-shadow: 0 1px 0 #FFFFFF;
}
.flat_area p
{
    color: #666666;
    line-height: 20px;
    margin: 0 0 10px;
    padding: 0 10px;
    text-shadow: 0 1px 0 #FFFFFF;
}
div.main_container p > a
{
    border-bottom: 1px dashed #CCCCCC;
    font-weight: bold;
    color: #AB65B8;
    text-decoration: none;
}
div.main_container p > a:hover
{
    border-bottom: 1px dashed #000000;
    color: #000000;
}
.box
{
    border-radius: 3px 3px 3px 3px;
    opacity: 1;
    background-color: white;
    box-shadow: 0 2px 0 1px rgba(0, 0, 0, 0.03);
    margin-bottom: 15px;
    padding: 0;
    position: relative;
    z-index: 400;
    width: 98%;
    margin-left: 0.99%;
    margin-right: 0.99%;
}
div.main_container .box {
	position: relative;
	background-color: white;
	margin-bottom: 15px;
	padding: 0;
	z-index: 400;
	-webkit-box-shadow: 0 2px 0 1px rgba(0,0,0,0.03);
	-moz-box-shadow: 0 2px 0 1px rgba(0,0,0,0.03);
	box-shadow: 0 2px 0 1px rgba(0,0,0,0.03);
}
div.main_container .box h2.box_head
{
    -moz-user-select: none;
    color: white;
    font-size: 14px;
    font-weight: bold;
    line-height: 32px;
    margin: -1px auto 0;
    text-indent: 10px;
    text-shadow: 0 1px 0 #000000;
    border-radius: 3px 3px 0 0;
    background-color: #9B6CA6;
    background-image: -moz-linear-gradient(center top , #9B6CA6, #8D579A);
    border-color: #8D579A;
}
.box.light h2.box_head,.box.light h2.box_head small {
	border-color: #959BA2;
	color: #333 !important;
	text-shadow: 0 1px 0 #FFFFFF !important;
}

.box.light a.toggle {
	background-image: url("../../Content/themes/base/images/lite_toggle.png");
}

.box h2.box_head small {
	font: normal 11px sans-serif;
	color: #ddd;
}

.block h2 small {
	font: normal 12px sans-serif;
	color: #aaa;
	letter-spacing: normal;
	margin-left: 5px;
}
.box a.toggle
{
    background: url("images/toggle.png") no-repeat scroll 1px -15px rgba(0, 0, 0, 0.1);
    border: 1px solid rgba(0, 0, 0, 0.2);
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 1px 0 rgba(255, 255, 255, 0.3);
    display: block;
    height: 17px;
    position: absolute;
    right: 33px;
    top: 6px;
    width: 18px;
    text-decoration: none;
}

.box a.toggle_closed {
	background-position: 2px 2px;
}
.box a.grabber
{
    background: url("images/grabber.gif") no-repeat;
	background: url("images/grabber-trans.png") no-repeat;
	display: block;
	height: 22px;
	width: 22px;
	position: absolute;
	right: 5px;
	top: 4px;
	cursor: move;
}
.box .block {
	padding-bottom:10px;
	margin-top: 0px;
	position: relative;
	background: none;
	overflow: hidden;
	border: 1px solid #ccc;
	border-width: 0 1px 1px;
}

.box.no_titlebar .block {
	border-width: 1px;
}

.box .block.tab_sider {
	border-width: 1px 1px 1px 0;
}

.box .block .section {
	padding: 15px;
	margin: 0;
}

.box .block .section:last-child {
}

.box .block h1.section,.box .block h2.section,.box .block h3.section {
	padding-bottom: 15px;
}

.box .block ul.flat_large {
	border: none;
	margin-top: 0px;
	margin-bottom: 0px;
	padding: 0;
}

.box .block ul.flat_large li {
	margin: 0 0 0 1px;
	margin-left: 0;
	border-bottom: 1px solid #eee;
	width: 100%x;
	padding: 10px 10px;
	list-style: none;
	display: block;
}

.box .block ul.flat_large li:last-child {
	border: none;
}

.box .block ul.flat_large li span {
	color: #333;
	display: block;
	float: left;
	width: 95px;
	font-weight: bold;
	font-size: 14px;
}
fieldset > label
{
    color: #333333;
    display: block;
    font-size: 13px;
    font-weight: bold;
    margin-right: 10px;
    padding: 0 20px 10px 20px;
}
fieldset > label span
{
    color: #999999;
    display: inline;
    font-family: sans-serif;
    font-size: 11px;
    font-weight: normal;
    margin-left: 5px;
}
fieldset
{
    border: 1px solid #DDDDDD;
    clear: both;
    position: relative;
    margin: 0;
    padding:0;
}
fieldset > div > .required_tag {
	background: url(../../Content/themes/base/images/input_required.png) top right;
	width: 40px;
	height: 40px;
	display: block;
	position: absolute;
	right: 0px;
	top: 0px;
}

fieldset.label_side{
}

fieldset.label_side input[type="text"]
{
    width: 100% !important;
}
fieldset.label_side input[type="password"]
{
    width: 100% !important;
}

fieldset.label_side > label {
	width: 100px;
	position: relative;
	float: left;
	left: 0;
	padding: 18px 20px 8px;
	border-right: 1px solid #eee;
	clear: left;
	line-height: normal;
}

fieldset.label_side > label > span {
	display: block;
	margin: 3px 0 -3px;
}

fieldset.label_side > label > span > a {
	color: #999;
}

fieldset.label_side > div {
	width: auto;
	margin-left: 140px;
	padding: 15px 20px;
	border-left: 1px solid #eee;
	clear: right;
}

fieldset.label_side > div > input.text {
	width: 100% !important;
}

fieldset.label_side.label_small > label {
	width: 70px;
}

fieldset.label_side.label_small > div {
	margin-left: 110px;
}

fieldset.no_label > div {
	padding: 15px 20px;
}

fieldset > div > label {
	display: block;
	line-height: 25px;
}

fieldset > div > label:first-child {
	margin-top: -3px;
}

fieldset > div.inline > label {
	display: inline-block;
	float: left;
	line-height: 26px;
	margin-right: 10px;
	margin-top: 0px;
}

fieldset > div > label input[type=radio],fieldset > div > label input[type=checkbox] {
	margin-right: 8px;
}
input[type="text"]
{
    height: 32px;
    line-height: 32px;
    padding: 0 0 0 5px;
    width: 50% !important;
}
input[type="password"]
{
    height: 32px;
    line-height: 32px;
    padding: 0 0 0 5px;
    width: 50% !important;
}
input[type="text"],input[type="password"], .textarea, .ui-multiselect, table input[type="text"]
{
    -moz-box-sizing: border-box;
    border-radius: 2px 2px 2px 2px;
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0.05);
    border: 0 none;
    box-shadow: 0 0 2px 0 #DDDDDD inset;
}
input, textarea
{
    font: 12px/1.5 'Helvetica Neue' ,Arial, 'Liberation Sans' ,FreeSans,sans-serif;
}
.textarea_holder, .textarea
{
    min-height: 60px;
    padding: 5px 0 0 5px;
    width: 100% !important;
}
select { border:solid 1px #9a9fa3; -moz-border-radius:3px; -webkit-border-radius:3px; border-redius:3px; height:24px; line-height:20px; width:50%; background:transparent url("images/select-bg.jpg") repeat-x; }
fieldset .required_tag
{
    background: url("images/input_required.png") repeat scroll right top transparent;
    display: block;
    height: 40px;
    position: absolute;
    right: 0px;
    top: 0px;
    width: 40px;
}
.required_tag img 
{ 
    display:none;
    position: absolute;
    right: 35px;
    top: 5px;
}

button
{
    border: 1px solid #000000;
    color: #FFFFFF;
    float: left;
    margin-bottom: 5px;
    margin-right: 5px;
    min-height: 30px;
    min-width: 30px;
    padding: 0 0;
    position: relative;
    text-shadow: 0 1px 0 #000000;
    
    background: #9B6CA6; /* for non-css3 browsers */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#9B6CA6', endColorstr='#8D579A'); /* for IE */
    background: -webkit-gradient(linear, left top, left bottom, from(#9B6CA6), to(#8D579A)); /* for webkit browsers */
    background: -moz-linear-gradient(top,  #9B6CA6,  #8D579A); /* for firefox 3.6+ */ 
    
    border-radius: 3px 3px 3px 3px;
}
button.green span
{
    line-height: 28px;
    padding-left: 5px;
    padding-right: 5px;
    width: auto;
}
button:hover
{
    background-color: #9b6ca6;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#8d579a), to(#9b6ca6)); /* Saf4+, Chrome */
    background-image: -webkit-linear-gradient(top, #8d579a, #9b6ca6); /* Chrome 10+, Saf5.1+, iOS 5+ */
    background-image: -moz-linear-gradient(top, #8d579a, #9b6ca6); /* FF3.6 */
    background-image: -ms-linear-gradient(top, #8d579a, #9b6ca6); /* IE10 */
    background-image: -o-linear-gradient(top, #8d579a, #9b6ca6); /* Opera 11.10+ */
    background-image: linear-gradient(top, #8d579a, #9b6ca6);
    border-color: #8d579a;
}
.grid
{
    margin: 0 20px;
}
.grid button > span
{
    padding: 0;
}
.form-item { border-top:solid 1px #dddddd;  padding:15px 20px; position:relative; overflow:hidden; }
.form-button { padding:15px 20px; overflow:hidden; }
.form-item label { color: #333333; display: block; font-size: 13px; font-weight: bold; margin-right: 10px; padding-bottom:10px;}

/* Styles for validation helpers
-----------------------------------------------------------*/
.field-validation-error
{
    color: #ff0000;
}
.field-validation-valid
{
    display: none;
}
.input-validation-error
{
    border: 1px solid #ff0000;
    background-color: #ffeeee;
}
.validation-summary-errors
{
    font-weight: bold;
    color: #ff0000;
    margin:20px 20px 0 20px;
}
.validation-summary-valid
{
    display: none;
}
div.validation-summary-errors ul
{
    background:url("../../Content/images/error.png") no-repeat scroll 9px 9px #FAD9D3;
    border:1px solid #E76757;
    color:#BF4329;
    margin:0 0 1em;
    padding:8px 24px 8px 34px;
}
.employee-link
{
    color:White;
    text-decoration:none;
}
.employee-details
{
    padding:.8em .6em;
    display:inline-block;
    cursor:pointer;
}
.employee-details
{
    display:block;
}
.employee-details:hover
{
    background-color:#666666;
}        
.employee-details:after
{
    content:"";
    display:block;
    clear:both;
}        
.employee-details dl,
.employee-details dt,
.employee-details dd
{
    margin:0;
    padding:0;
}        
.employee-details img,
.employee-details h3,
.employee-details dl,
.commands
{
    float:left;
    clear:none;
    margin:0 14px 0 0;
}
.employee-details h3,
.employee-details dl
{
    width:230px;
    min-height:115px;
    border-width:0 1px 0 0;
    background-color:transparent;
    color:inherit;
}        
.employee-details h3
{
    width:90px;
    padding:0;
    font-size:1.5em;
    padding-right:20px;
}        
.employee-details dt
{
    font-weight:bold;
}
