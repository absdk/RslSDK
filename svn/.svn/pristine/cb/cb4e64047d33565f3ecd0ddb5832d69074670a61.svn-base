.search-item-left { float:left; width:47%; padding:15px 20px; }
.search-item-left .label-container { float:left; width:25%; padding-top:10px; }
.search-item-left label { color: #333333; display: block; font-size: 13px; font-weight: bold; margin-right: 10px; padding-top:8px;}
