@model SoftwareGrid.DataLogic.Models.SalesProductServiceCatelog

@{
    ViewBag.Title = "Sales Product Service Catelog";
}
<div class="flat_area grid_16">
    <h2>
        Sales Product Service Catelog</h2>
</div>
<div class="box grid_16">
    <div class="block">
        <h2 class="box_head">
            Are you sure you want to delete this?</h2>
@using (Html.BeginForm()) {
	<fieldset>
				<div class="form-item">
		    <label>Title</label>
		    <div class="display-field">@Model.Title</div>
		</div>
				<div class="form-item">
		    <label>Description</label>
		    <div class="display-field">@Model.Description</div>
		</div>
				<div class="form-item">
		    <label>SalesProductServiceCategory</label>
		    <div class="display-field">@(Model.SalesProductServiceCategory == null ? "None" : Model.SalesProductServiceCategory.Name)</div>
		</div>
				<div class="form-item">
	<div class="button_bar clearfix">
			<button class="green" type="submit">
				<span>Delete</span>
			</button>
		</div>
	</div>
	</fieldset>
}
	</div>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
	
