@model SoftwareGrid.DataLogic.Models.OrganizationBankInfo

@{
    ViewBag.Title = "OrganizationBankInfo";
    Layout = "~/Views/Shared/_LayoutOrganization.cshtml";
}

<div class="flat_area grid_16">
    <h2>
        Organization Bank Info</h2>
</div>

<div class="box grid_16">
    <div class="block">
        <h2 class="box_head">
            Organization Bank Info</h2>
		<fieldset>
<div class="form-item">
    @Html.LabelFor(model => model.BankName)
    <div class="display-field">@Model.BankName</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.BankAddress)
    <div class="display-field">@Model.BankAddress</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.POBox)
    <div class="display-field">@Model.POBox</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.BankCity)
    <div class="display-field">@Model.BankCity</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.ZipCode)
    <div class="display-field">@Model.ZipCode</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.ComanyBankAccountNumber)
    <div class="display-field">@Model.ComanyBankAccountNumber</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.BankAccountType)
    <div class="display-field">@Model.BankAccountType</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.SwipeCode)
    <div class="display-field">@Model.SwipeCode</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.RoutingNo)
    <div class="display-field">@Model.RoutingNo</div>
</div>
<div class="form-item">
    @Html.LabelFor(model => model.Organization)
    <div class="display-field">@(Model.Organization == null ? "None" : Model.Organization.Name)</div>
</div>
		</fieldset>
		<div class="form-item">
	@Html.ActionLink("Edit", "Edit", new { id = Model.OrganizationBankInfoID, organizationId = ViewBag.OrganizationId }) |
	@Html.ActionLink("Back to List", "Index",new { organizationId = ViewBag.OrganizationId })
    
</div>
	</div>
</div>



