@model SoftwareGrid.DataLogic.Models.ApplicantDataPrivacyTemplate

@{
    ViewBag.Title = "Applicant Data Privacy Template";
}
<div class="flat_area grid_16">
    <h2>
        Applicant Data Privacy Template</h2>
</div>
<div class="box grid_16">
    <div class="block">
        <h2 class="box_head">
            Are you sure you want to delete this?</h2>
@using (Html.BeginForm()) {
	<fieldset>
				<div class="form-item">
		    <label>AgreementTitle</label>
		    <div class="display-field">@Model.AgreementTitle</div>
		</div>
				<div class="form-item">
		    <label>DataPrivacyAgreementInfo</label>
		    <div class="display-field">@Model.DataPrivacyAgreementInfo</div>
		</div>
				<div class="form-item">
		    <label>IsCurrent</label>
		    <div class="display-field">@Model.IsCurrent</div>
		</div>
				<div class="form-item">
	<div class="button_bar clearfix">
			<button class="green" type="submit">
				<span>Delete</span>
			</button>
		</div>
	</div>
	</fieldset>
}
	</div>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
	
